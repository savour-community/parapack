{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@routes/*": ["./routes/*"],
      "@screen/*": ["./screen/*"],
      "@hooks/*": ["./hooks/*"],
      "@api/*": ["./api/*"],
      "@assets/*": ["./assets/*"],
      "@common/*": ["./common/*"],
      "@i18n/*": ["./i18n/*"],
      "@components/*": ["./components/*"],
    },
    "types": ["redux-thunk"]
  }
}
